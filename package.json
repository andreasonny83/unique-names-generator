{
  "name": "unique-names-generator",
  "version": "1.0.13",
  "description": "Generate unique and memorable names",
  "engines": {
    "node": ">=4"
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run build",
    "test": "npm run format && npm run lint && jest",
    "test:watch": "jest --watchAll",
    "format": "prettier --write src/**/*.js",
    "prebuild": "npm run format",
    "build": "rimraf lib/ && babel ./src --out-dir lib/ --ignore **/*.test.js --copy-files",
    "prerelease": "npm run build",
    "release": "np --no-yarn"
  },
  "main": "index.js",
  "tags": [
    "name-generator",
    "unique-names"
  ],
  "files": [
    "index.js",
    "lib/unique-names-generator.js",
    "lib/dictionaries"
  ],
  "author": "AndreaSonny <andreasonny83@gmail.com>",
  "license": "MIT",
  "private": false,
  "repository": {
    "type": "git",
    "url": "git@github.com:andreasonny83/unique-names-generator.git"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-eslint": "^10.0.1",
    "babel-preset-env": "^1.7.0",
    "eslint": "^5.7.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-config-babel": "^8.0.1",
    "eslint-plugin-flowtype": "^2.50.3",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jest": "^21.24.1",
    "jest": "^23.6.0",
    "np": "^3.0.4",
    "prettier": "^1.14.3"
  }
}
