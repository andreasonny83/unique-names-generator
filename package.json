{
  "name": "unique-names-generator",
  "version": "3.1.1",
  "description": "Generate unique and memorable names",
  "engines": {
    "node": ">=6"
  },
  "scripts": {
    "lint": "eslint --ext .ts ./src",
    "test": "npm run format && npm run lint && jest --coverage",
    "test:watch": "jest --coverage --watchAll",
    "format": "prettier --write src/**/*.ts",
    "prebuild": "rimraf dist && npm run format",
    "build": "microbundle",
    "prepublishOnly": "npm run build",
    "prerelease": "npm run build",
    "contributors:add": "all-contributors add",
    "contributors:generate": "all-contributors generate",
    "release": "np"
  },
  "main": "dist/index.js",
  "umd:main": "dist/index.umd.js",
  "module": "dist/index.m.js",
  "source": "src/index.ts",
  "types": "dist/index.d.ts",
  "tags": [
    "name-generator",
    "unique-names",
    "typescript"
  ],
  "files": [
    "dist"
  ],
  "author": "AndreaSonny <andreasonny83@gmail.com>",
  "license": "MIT",
  "private": false,
  "repository": {
    "type": "git",
    "url": "git@github.com:andreasonny83/unique-names-generator.git"
  },
  "devDependencies": {
    "@babel/core": "^7.7.2",
    "@babel/preset-env": "^7.7.1",
    "@babel/preset-typescript": "^7.7.2",
    "@types/jest": "^24.0.22",
    "@typescript-eslint/eslint-plugin": "^2.7.0",
    "@typescript-eslint/parser": "^2.7.0",
    "all-contributors-cli": "^6.10.0",
    "babel-jest": "^24.9.0",
    "eslint": "^6.6.0",
    "eslint-config-prettier": "^6.5.0",
    "eslint-plugin-prettier": "^3.1.1",
    "jest": "^24.9.0",
    "microbundle": "^0.11.0",
    "np": "^5.1.3",
    "prettier": "^1.19.1",
    "prettier-eslint": "^9.0.0",
    "rimraf": "^3.0.0",
    "typescript": "^3.7.2"
  }
}
